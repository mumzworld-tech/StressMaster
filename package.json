{
  "name": "stressmaster",
  "version": "1.0.3",
  "description": "AI-powered load testing CLI tool using natural language commands. Convert plain English test descriptions into K6 load test scripts.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "stressmaster": "./dist/cli.js",
    "sm": "./dist/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "build:clean": "rm -rf dist && npm run build",
    "dev": "tsx src/cli.ts",
    "start": "node dist/cli.js",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:unit": "vitest run --config vitest.unit.config.ts",
    "test:integration": "vitest run --config vitest.integration.config.ts",
    "test:e2e": "vitest run tests/e2e/",
    "test:watch": "vitest",
    "test:unit:watch": "vitest --config vitest.unit.config.ts",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "typecheck": "tsc --noEmit",
    "validate": "npm run typecheck && npm run lint && npm run test",
    "security:audit": "npm audit --audit-level=moderate"
  },
  "keywords": [
    "load-testing",
    "stress-testing",
    "performance-testing",
    "api-testing",
    "k6",
    "ai",
    "cli",
    "natural-language",
    "testing",
    "load-tester",
    "qa",
    "automation"
  ],
  "author": "StressMaster Contributors",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/mumzworld-tech/StressMaster.git"
  },
  "bugs": {
    "url": "https://github.com/mumzworld-tech/StressMaster/issues"
  },
  "homepage": "https://github.com/mumzworld-tech/StressMaster#readme",
  "files": [
    "dist/",
    "README.md",
    "LICENSE"
  ],
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "dependencies": {
    "@types/dockerode": "^3.3.42",
    "@types/js-yaml": "^4.0.9",
    "axios": "^1.6.0",
    "chalk": "^4.1.2",
    "cli-table3": "^0.6.3",
    "commander": "^11.1.0",
    "dockerode": "^4.0.7",
    "dotenv": "^17.2.1",
    "inquirer": "^9.2.0",
    "joi": "^17.11.0",
    "js-yaml": "^4.1.0",
    "rxjs": "^7.8.0",
    "uuid": "^9.0.0",
    "ws": "^8.14.0"
  },
  "devDependencies": {
    "@types/inquirer": "^9.0.0",
    "@types/node": "^20.8.0",
    "@types/uuid": "^9.0.0",
    "@types/ws": "^8.5.0",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "@vitest/coverage-v8": "^0.34.0",
    "eslint": "^8.50.0",
    "tsx": "^3.14.0",
    "typescript": "^5.2.0",
    "vitest": "^0.34.0"
  }
}
